{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self'; object-src 'none'; style-src 'self';"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "DEV_WEBSOCKET_URL": "@dev_websocket_url",
    "PREVIEW_WEBSOCKET_URL": "@preview_websocket_url",
    "PROD_WEBSOCKET_URL": "@prod_websocket_url",
    "DEV_HOME_URL": "@dev_home_url",
    "PROD_HOME_URL": "@prod_home_url"
  }
}
